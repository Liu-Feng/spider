<script src="js/jquery-1.11.1.min.js"></script>
<script src="js/elasticsearch-js/elasticsearch.jquery.min.js"></script>
<script src="js/utils.js"></script>
<script>
$(document).ready(function(){
  var objDate = new Date();
	$("#spanDate").text(objDate.toLocaleDateString());

	var client = $.es.Client({
		hosts: '172.20.8.3:9200',
		log: "trace"
	});

	var searchKeywords = function(cateId,keyword,indexName,typeName,num){
	  var query = '';
		var orderStr = 'publishtime:desc';
		var bodyStr = '{query:{match_all:{}}}'
	  if(cateId == 'jtdt-gfxx'){
			bodyStr = '{query:{match:{sitename:"cnddr"}}}'
	  }else if(cateId == 'jtdt-mttg'){
			bodyStr = '{query:{match:{sitename:"baidu"}}}'
	  }else if(cateId == 'dsdt-mdl'){
			bodyStr = '{query:{match:{sitename:"mcdonalds"}}}'
	  }else if(cateId == 'dsdt-kfc'){
			bodyStr = '{query:{match:{sitename:"kfc"}}}'
	  }else if(cateId == 'dsdt-jyj'){
			bodyStr = '{query:{match:{sitename:"yos"}}}'
	  }else if(cateId == 'dsdt-yhdw'){
			bodyStr = '{query:{match:{sitename:"yonghe"}}}'
	  }else{
			bodyStr = '{query:{match_all:{}}}'
	  }

	  if(keyword == ''){
	    alert("请输入关键词！");	
	  }else{
	    client.search({
	      index:indexName,
	      type:typeName,
	      size:num,
	      //q:query,
				body:bodyStr,
	      sort:orderStr
	    }).then(function(json){
	      $("#"+cateId+" tbody tr").remove();
	      if(json.hits.total == 0){
		//alert('没有您要搜索的数据！');
	      }else{
		$.each(json.hits.hits,function(i,v){
		  data = v._source
		  dataHighLight = v.highlight
		  if(cateId == 'sjmt-weibo'){
		    var userUrl = data.user_url

		    var screenName = data.screen_name
		    var content = data.content
		    var strNewRow = '<table width="85%" align="center"  border="0" cellspacing="0" cellpadding="0"  > <tr> <td  width="64"  style="float:left;padding-top:10px;"valign="top" ><img src="'+data.avatarimg+'" width="54" height="54" /></td> <td width="591" style="float:left padding-left:30px;font-size:14px;padding-top:10px;font-family:微软雅黑;" valign="top"> <a style="color: #006A92;font-weight: bold;" href="' + userUrl + '">' + screenName + '</a> <span>' + content + '</span> <table width="100%" border="0" cellspacing="0" cellpadding="0"> <tr> <td align="left" style="color: #7195A7;font-size:11px; "> 来自新浪微博</td> <td align="right" style="color: #7195A7;font-size:11px;"></td> </tr> </table></td> </tr> </table>'
		    $("#"+cateId+"").append(strNewRow);
			}else if(cateId == 'sjmt-weixin'){
		    var title = data.title
		    var sitename = '微信'
		    var publishtime = data.publishtime
		    var url = data.url
		    var strNewRow = '<tr><td style="padding-left:40px; "><li><a target="_blank" href="'+url+'"><span style="color:#006A92;font-size:15px;font-family:微软雅黑;">'+title+'</span></a><span style="color:#888888;font-size:11px;font-family:微软雅黑;"> '+sitename+'</span></li></td></tr>'
		    $("#"+cateId+" tbody").append(strNewRow);
			}else if(cateId == 'hyhd'){
		    var title = data.title
		    var sitename = data.sitename
		    var timeStr = data.time
		    var url = data.url
		    //var strNewRow = '<tr><td style="padding-left:40px; "><li><a target="_blank" href="'+url+'"><span style="color:#006A92;font-size:15px;font-family:微软雅黑;">'+title+'</span></a><span style="color:#888888;font-size:11px;font-family:微软雅黑;"> '+sitename+'</span></li></td></tr>'
				var strNewRow = '<tr> <td style="padding-left:40px; "><li><a href="'+url+'"> <span style="color:#006A92;font-size:15px;font-family:微软雅黑;">'+title+'</span></a> <span style="color:#888888;font-size:11px;font-family:微软雅黑;"> '+sitename+'</span> <span style="color:#888888;font-size:11px;font-family:微软雅黑;"> '+timeStr+'</span></li> </td> </tr>'
		    $("#"+cateId+" tbody").append(strNewRow);
		  }else{
		    var title = data.title
		    var sitename = data.sitename
		    var publishtime = (data.publishtime).replace("T"," ")
		    var url = data.url
		    var strNewRow = '<tr><td style="padding-left:40px; "><li><a target="_blank" href="'+url+'"><span style="color:#006A92;font-size:15px;font-family:微软雅黑;">'+title+'</span></a><span style="color:#888888;font-size:11px;font-family:微软雅黑;"> '+sitename+'</span></li></td></tr>'
		    $("#"+cateId+" tbody").append(strNewRow);
		  }


		});
	      }
	    }, function(err){
	      console.trace(err.message);
	    });
	  }
	};

	arrCate = new Array(
		"jtdt-gfxx",
		"jtdt-mttg",
		"dsdt-mdl",
		"dsdt-kfc",
		"dsdt-jyj",
		"dsdt-yhdw",
		"sjmt-weibo",
		"sjmt-weixin"
	);
	arrKeywords = new Array(
		"_all",
		"_all",
		"_all",
		"_all",
		"_all",
		"_all",
		"_all",
		"_all"
	)

	var indexName = "daniangshuijiao";
	var typeName = "";
	for (var i = 0; i<arrCate.length; i++) {
		console.log(arrCate[i] + "##" + arrKeywords[i]);
		if (arrCate[i] == 'jtdt-gfxx' || arrCate[i] == 'jtdt-mttg' || arrCate[i] == 'dsdt-mdl' || arrCate[i] == 'dsdt-kfc' || arrCate[i] == 'dsdt-jyj' || arrCate[i] == 'dsdt-yhdw') {
			typeName = 'article';
		} else if (arrCate[i] == 'sjmt-weibo') {
			typeName = 'weibo';
		} else if (arrCate[i] == 'sjmt-weixin') {
			typeName = 'weixin';
		} else {
			typeName = 'article';
		};
		searchKeywords(arrCate[i],arrKeywords[i],indexName,typeName,6);	
		
	};
	
	
});
</script>
